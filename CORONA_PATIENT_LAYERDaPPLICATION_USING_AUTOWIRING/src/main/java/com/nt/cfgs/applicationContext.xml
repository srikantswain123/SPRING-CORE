  </bean>
  
  
  
  <bean id="oraDao" class="com.nt.dao.OraPatientDAOImpl" autowire="constructor" primary="true"/>
  
 
  
  <bean id="patientService" class="com.nt.service.CorornaPatientMGmtServiceImpl" autowire="constructor"/>


  
  <bean id="mco" class="com.nt.controler.MainControler" autowire="constructor"/>
  

